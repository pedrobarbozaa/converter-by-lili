{
  "expo": {
    "name": "Calculadora da Ligia",
    "slug": "calculadora-ligia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/android-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ccd5ae"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ligia.calculadora",
      "buildNumber": "1",
      "infoPlist": {
        "UILaunchStoryboardName": "SplashScreen"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-icon.png",
        "backgroundColor": "#ccd5ae"
      },
      "package": "com.ligia.calculadora",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ccd5ae",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "857f84d6-6b05-438f-8f04-fd5e5a4e9917"
      }
    }
  }
}
